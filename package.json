    "autoprefixer": "^10.4.13",
    "eslint": "^8.0.1",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
